{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/types",
    "packages/core",
    "packages/react",
    "packages/components",
    "packages/fields",
    "packages/layout"
  ],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "name": "ObjectUI API Reference",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "exclude": [
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.stories.ts",
    "**/*.stories.tsx",
    "**/*.mdx"
  ],
  "categoryOrder": [
    "Schema",
    "Components",
    "Hooks",
    "Utilities",
    "Types",
    "*"
  ],
  "categorizeByGroup": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "githubPages": false,
  "hideGenerator": true
}
