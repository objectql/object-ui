{
  "name": "@object-ui/console-server",
  "version": "1.0.0",
  "description": "ObjectStack Console Server Plugin - Serves the ObjectUI Console as a Hono middleware",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "client",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:client": "node scripts/copy-client.js",
    "build:all": "pnpm build && pnpm build:client",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist client",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "dependencies": {
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public"
  }
}
